{
  "name": "KiRa-KiRa",
  "version": "1.0",
  "manifest_version": 2,
  "description": "KiRa-KiRa",
  "icons": {"128": "icon128.png"},
  "background": {
    "scripts": ["zepto.min.js", "eventPage.js"],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "webRequest", "http://*/*"
  ]
}
